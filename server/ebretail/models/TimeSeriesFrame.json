{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "TimeSeriesFrame": {
      "title": "TimeSeriesFrame",
      "description": "This object represents a MultiCameraFrame after it has been processed by time-series analysis with previous frames.",
      "type": "object",
      "properties": {
        "storeId": {
          "type": "number"
        },
        "timestamp": {
          "type": "string"
        },
        "people": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "visitorId": {
                "type": "string"
              },
              "state": {
                "type": "string",
                "enum": ["entered", "visible", "hidden", "exited"]
              },
              "cameraIds": {
                "type": "array",
                "items": {"type": "string"}
              },
              "x": {
                "type": "number"
              },
              "y": {
                "type": "number"
              },
              "zone": {
                "type": "number"
              }
            }
          }
        }
      }
    }
  },
  "$ref": "#/definitions/TimeSeriesFrame"
}